<?xml version="1.0" encoding="UTF-8"?>

<KASP>
<Policy name="Policy1">
	<Description>
			Policy1 in ODS wiki BasicTest outline
	</Description>
		
	<Signatures>
		<Resign>PT4S</Resign>
		<Refresh>PT5S</Refresh>
		<Validity>
			<Default>PT15S</Default>
			<Denial>PT15S</Denial>
		</Validity>
		<Jitter>PT2S</Jitter>
		<InceptionOffset>PT1S</InceptionOffset>
	</Signatures>
	<Denial>
		<NSEC/>
	</Denial>
	
	<Keys>
		<!-- Parameters for both KSK and ZSK -->
		<TTL>PT20S</TTL>
		<RetireSafety>PT15S</RetireSafety>
		<PublishSafety>PT1S</PublishSafety>
		<Purge>PT3H</Purge>
		<!-- Parameters for KSK only -->
		<KSK>
			<Algorithm length="2048">7</Algorithm>
			<Lifetime>PT50M</Lifetime>
			<!-- @TODO@ Repository should be configured -->
			<Repository>SoftHSM</Repository>
			<!--<Standby>1</Standby>-->
		</KSK>
		<!-- Parameters for ZSK only -->
		<ZSK>
			<Algorithm length="2048">7</Algorithm>
			<Lifetime>PT10M</Lifetime>
			<!-- @TODO@ Repository should be configured -->
			<Repository>SoftHSM</Repository>
			<!--<Standby>1</Standby>-->
		</ZSK>
	</Keys>
	
	<Zone>
		<PropagationDelay>PT0S</PropagationDelay>
		<SOA>
			<TTL>PT0M</TTL>
			<Minimum>PT0M</Minimum>
			<Serial>unixtime</Serial>
		</SOA>
	</Zone>
	
	<Parent>
		<PropagationDelay>PT1M</PropagationDelay>
		<DS>
			<TTL>PT10S</TTL>
		</DS>
		<SOA>
			<TTL>PT0M</TTL>
			<Minimum>PT0M</Minimum>
		</SOA>
	</Parent>
</Policy>


